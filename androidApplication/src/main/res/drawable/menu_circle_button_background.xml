<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:state_pressed="false">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
            <stroke android:width="2dp" android:color="@color/control_dark" />
            <solid android:color="@color/control_text_button_disable_bg" />
        </shape>
    </item>
    <item android:state_enabled="true" android:state_pressed="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
            <stroke android:width="2dp" android:color="@color/control_dark" />
            <solid android:color="@color/control_text_button_bg" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
            <stroke android:width="1dp" android:color="@color/control_text_button_bg" />
            <solid android:color="@color/control_text_button_disable_bg" />
        </shape>
    </item>
</selector>
